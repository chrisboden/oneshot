# Copy this file to .env and add your actual API keys

# OpenRouter API Key (required)
# Get from: https://openrouter.ai/keys
OPENROUTER_API_KEY="your_openrouter_api_key_here"

# Brave Search API Key (optional - for web search functionality)
# Get from: https://api.search.brave.com/app/keys
BRAVE_API_KEY="your_brave_api_key_here"

# Logfire Tokens (optional - for logging/monitoring)
# Get from: https://logfire.pydantic.dev/
LOGFIRE_WRITE_TOKEN="your_logfire_write_token_here"
LOGFIRE_READ_TOKEN="your_logfire_read_token_here"

# Hugging Face Token (optional - for some AI models)
# Get from: https://huggingface.co/settings/tokens
HUGGINGFACE_INFERENCE_TOKEN="your_huggingface_token_here"