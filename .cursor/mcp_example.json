{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ]
    },
    "logfire": {
      "command": "uvx",
      "args": [
        "logfire-mcp@latest",
        "--read-token=PASTE_YOUR_LOGFIRE_READ_TOKEN_HERE"
      ]
    },
    "oneshot": {
      "command": "python3",
      "type": "stdio",
      "args": [
        "/Users/PATH_TO_ONESHOT_REPO/oneshot/oneshot_mcp.py"
      ]
    }
  }
}