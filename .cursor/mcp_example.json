{
  "mcpServers": {
    "oneshot": {
      "command": "python3",
      "type": "stdio",
      "args": ["/Users/PATH/TO/oneshot/app/oneshot_mcp.py"]
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ]
    },
    "logfire": {
      "command": "uvx",
      "args": ["logfire-mcp@latest", "--read-token=LOGFIRE_READ_TOKEN_GOES_HERE"]
    }
  }
}